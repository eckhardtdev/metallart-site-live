{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["./src/**/*.d.ts", "./src/**/*.ts", "./src/**/*.astro"],
  "exclude": ["./node_modules", "./dist"]
}
