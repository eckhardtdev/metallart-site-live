{
  "name": "metallart-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "dev:ssl": "SECURE=on astro dev --host",
    "start": "astro dev --host --open",
    "check": "astro check",
    "build": "astro check && astro build",
    "serve": "netlify serve",
    "preview": "astro preview",
    "astro": "astro",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "sb:login": "storyblok logout && storyblok login",
    "sb:pull": "storyblok pull-components --space 273160 --path ./storyblok/ && storyblok-generate-ts source=./storyblok/components.273160.json target=./src/types/storyblok.d.ts",
    "sb:push": "node scripts/prompt-sb-push.js",
    "sb:pull-seperate": "storyblok pull-components --space 273160 --separate-files --file-name production --path ./storyblok/components/",
    "ssl": "local-ssl-proxy --source 4000 --target 4321 --cert localhost.pem --key localhost-key.pem",
    "ssl:netlify": "local-ssl-proxy --source 8000 --target 8888 --cert localhost.pem --key localhost-key.pem"
  },
  "dependencies": {
    "@astrojs/netlify": "^5.2.0",
    "@astrojs/tailwind": "^5.1.0",
    "@fontsource-variable/sora": "^5.0.17",
    "@tailwindcss/typography": "^0.5.11",
    "astro": "^4.5.10",
    "clsx": "^2.1.0",
    "lodash": "^4.17.21",
    "marked": "^12.0.1",
    "normalize-url": "^8.0.1",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/alpinejs": "^3.13.10",
    "@types/alpinejs__collapse": "^3.13.4",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "form-data-encoder": "^4.0.2",
    "formdata-node": "^6.0.3",
    "image-size": "^1.1.1",
    "jsdom": "^24.0.0",
    "local-ssl-proxy": "^2.0.5",
    "mime": "^4.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "storyblok": "^3.29.1",
    "vitest": "^1.4.0"
  }
}
