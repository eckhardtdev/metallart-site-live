{
  "name": "metallart-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev --host --open",
    "check": "astro check",
    "build": "astro check && astro build",
    "serve": "netlify serve",
    "astro": "astro",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "sb:login": "storyblok logout && storyblok login",
    "sb:pull": "storyblok pull-components --space 273160 --path ./storyblok/ && storyblok-generate-ts source=./storyblok/components.273160.json target=./src/types/storyblok.d.ts",
    "sb:push": "node scripts/prompt-sb-push.js",
    "sb:pull-seperate": "storyblok pull-components --space 273160 --separate-files --file-name production --path ./storyblok/components/",
    "ssl": "local-ssl-proxy --source 4000 --target 4321 --cert localhost.pem --key localhost-key.pem",
    "ssl:serve": "local-ssl-proxy --source 8000 --target 8888 --cert localhost.pem --key localhost-key.pem",
    "license-compliance": "license-compliance"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.14.3",
    "@alpinejs/persist": "^3.14.3",
    "@astrojs/alpinejs": "^0.4.0",
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^5.5.4",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.2",
    "@fontsource-variable/sora": "^5.1.0",
    "@fontsource/sora": "^5.1.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@iconify-json/fa-brands": "^1.2.0",
    "@material-tailwind/react": "^2.1.10",
    "@sentry/astro": "^8.36.0",
    "@splidejs/splide": "^4.1.4",
    "@spotlightjs/astro": "^2.1.11",
    "@storyblok/astro": "^5.0.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "alpinejs": "^3.14.3",
    "astro": "^4.16.9",
    "astro-icon": "^1.1.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "clsx": "^2.1.1",
    "formik": "^2.4.6",
    "html-to-text": "^9.0.5",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "marked": "^14.1.3",
    "nanoid": "^5.0.8",
    "nodemailer": "^6.9.16",
    "normalize-url": "^8.0.1",
    "p-debounce": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "slugify": "^1.6.6",
    "storyblok-rich-text-astro-renderer": "^2.2.2",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/alpinejs": "^3.13.10",
    "@types/alpinejs__collapse": "^3.13.4",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.1",
    "form-data-encoder": "^4.0.2",
    "formdata-node": "^6.0.3",
    "image-size": "^1.1.1",
    "jsdom": "^25.0.1",
    "license-compliance": "^3.0.1",
    "local-ssl-proxy": "^2.0.5",
    "mime": "^4.0.4",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "storyblok": "^3.35.0",
    "storyblok-generate-ts": "^2.1.0",
    "vitest": "^2.1.4"
  }
}
